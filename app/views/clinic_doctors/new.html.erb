<%= form_with url: "/clinics/#{@clinic.id}/doctors", method: :post, local: true do |form| %>
  <%= form.label :doctor_name %>
  <%= form.text_field :name %>
  <br>
  <br>
  <%= form.label :rating %>
  <%= form.text_field :rating %>
 <br>
 <br>
 <%= form.label :available %>
 <%= form.text_field :available %>
 <br>
 <br>
  <%= form.submit "Create Doctor" %>
<% end %>
