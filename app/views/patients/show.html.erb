<%= link_to "Hospital Index", "/hospitals" %>
<%= link_to "Patient Index", "/patients" %>
<%= link_to "Clinic Index", "/clinics" %>
<%= link_to "Doctor Index", "/doctors" %>
<a href="/patients/:id"></a>
<h1>Patient</h1>

<h3>Name: <%= @patient.name %></h3>
<p>id: <%= @patient.id %></p>
<p>Visitors: <%= @patient.visitors %></p>
<p>Age: <%= @patient.age %></p>
<p>Hospital id: <%= @patient.hospital_id %></p>
<p>Created at: <%= @patient.created_at %></p>
<p>Updated at: <%= @patient.updated_at %></p>

<%= link_to "Update Patient #{@patient.name}", "/patients/#{@patient.id}/edit" %>
<br>
<%= link_to "Delete Patient #{@patient.name}", "/patients/#{@patient.id}", method: :delete %>
